<Page x:Class="Psytest.UI.Pages.TestingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Psytest.UI.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TestingPage">


    <ScrollViewer VerticalScrollBarVisibility="Visible">
        <WrapPanel HorizontalAlignment="Center" Orientation="Vertical">

            <StackPanel Name="MainStackPanel" Orientation="Vertical"/>

            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
                <Button Content="Завершить" Margin="10" x:Name="ButtonEnd" HorizontalAlignment="Center" Click="ButtonEnd_Click"/>
            </StackPanel>

        </WrapPanel>
    </ScrollViewer>
</Page>

    
    